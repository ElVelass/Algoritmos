	leer num
	seno= sen(num)
	coseno= cos(num)
	tangente=tan(num)
	raiz_2= raiz(num)
	logaritmo_natural=ln(num)
	Escribir" del num",num
	Escribir "su seno es",seno
	Escribir"su coseno es",coseno
	Escribir "su tangente es",tangente
